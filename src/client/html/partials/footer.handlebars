<div data-role="footer" data-position="fixed" class="jqm-footer" data-tap-toggle="false">
 

    
    <div class="social_icons">
        <ul>
            <!--Facebook-->
            <li>
            <style>
                          .fill-mycolor { fill: #3e465b; }
                          
                        </style>
                <a data-bind="attr:{href:'https://www.facebook.com/sharer/sharer.php?u='+url()+'&amp;t='+title()}"
                   class="popup social-fb social-btn" target="_top">
                    <svg version="1.1" class = "fill-mycolor" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                         viewBox="0 0 486.392 486.392" fill="#3e465b" width="32px" height="32px" style="enable-background:new 0 0 486.392 486.392;" xml:space="preserve">
                         <defs>
    <style type="text/css"><![CDATA[
        path{
        fill:red;
      }
    ]]></style>
  </defs>
                    <use xlink:href='{{{getImageNoCDN "sprites.svg#icon-fb"}}}'></use>
                    </svg>
                </a>
            </li>
            <!--google+-->
            <li>
               <style>
                          .fill-mycolor { fill: #3e465b; }
                          
                        </style>
                <a data-bind="attr:{href:'https://plus.google.com/share?url='+url()}"
                   class="popup social-gp social-btn" target="_top">
                    <svg version="1.1" class = "fill-mycolor" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                         width="32px" height="32px" viewBox="0 0 612 612" fill="#3e465b" style="enable-background:new 0 0 612 612;" xml:space="preserve">
                      
                    <use xlink:href='{{{getImageNoCDN "sprites.svg#icon-gp"}}}'></use>
                    </svg>
                </a>
            </li>
            <!--linkdin-->
            <li>
                <a data-bind="attr:{href:'http://www.linkedin.com/shareArticle?mini=true&amp;url='+url()+'&amp;title='+title()}"
                   class="popup social-ln social-btn" target="_top">
                    <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="32px" height="32px" viewBox="0 0 486.392 486.392" style="enable-background:new 0 0 486.392 486.392;" xml:space="preserve" fill="#3e465b">
                    <use xlink:href='{{{getImageNoCDN "sprites.svg#icon-ln"}}}'></use>
                    </svg>
                </a>
            </li>
            <!--Twitter-->
            <li>
                <a data-bind="attr:{href:'https://twitter.com/intent/tweet?text='+url()}"
                   target="_top" class="popup social-tw social-btn">
                    <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                         width="32px" height="32px" fill="#3e465b" viewBox="0 0 49.652 49.652" style="enable-background:new 0 0 49.652 49.652;"
                         xml:space="preserve">
                    <use xlink:href='{{{getImageNoCDN "sprites.svg#icon-tw"}}}'></use>
                    </svg>
                </a>
            </li>

        </ul>
    </div>
  
</div>